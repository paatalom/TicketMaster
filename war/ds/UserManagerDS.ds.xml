<DataSource ID="UserManagerDS"
			serverType="sql"
			tableName="ticketsdb.Users"
			qualifyColumnNames="false"
			dropExtraFields="false">
	<fields>
		<field name="id" 			type="integer" 		title="id" hidden="true" primaryKey="true" />
		<field name="USER_NAME" 		type="integer" 		title="USER_NAME" />
		<field name="Password" 			type="text" 		title="Password" />
	</fields>

	<operationBindings>

		<operationBinding operationId="getAllUsers"
			operationType="fetch">
			<selectClause>
				t.id,
				t.USER_NAME,
				t.Password
			</selectClause>
			<tableClause>
				ticketsdb.Users t
			</tableClause>
			<whereClause><![CDATA[
				1 = 1
			]]>
			</whereClause>
		</operationBinding>

	</operationBindings>
</DataSource>
